{
  "crawl_id": "food_dotcom",
  "url_instr": {
    "http://www.food.com/": {
      "url_filters": ["recipe"],
      "filter_instr": {
        "recipe": [
          {
            "label":"title",
            "selector":"span.item h1.fn"
          },
          {
            "label":"ingredients",
            "selector":"span.ingredient span.name"
          },
          {
            "label":"description",
            "selector":"p#usertextnote"
          },
          {
            "label":"rating",
            "selector":"ul.star-rating li.current-rating",
            "property":"style"
          },
          {
            "label":"photo",
            "selector":"div#enlrphoto img",
            "property":"src"
          }
        ]
      }
    }
  }
}

